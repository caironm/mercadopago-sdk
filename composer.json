{
    "name": "gpupo/mercadopago-sdk",
    "description": "SDK Não Oficial para integração a partir de aplicações PHP com APIs Mercadopago",
    "homepage": "https://opensource.gpupo.com/mercadopago-sdk/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["mercadopago", "marketplace", "sdk", "api"],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "ext-curl" : "*",
        "gpupo/common-sdk": "^4.6",
        "gpupo/common-schema": "^4.5.2",
        "symfony/yaml": "^4.2.2",
        "monolog/monolog": "^1.24",
        "symfony/finder": "^4.2.2",
        "symfony/cache": "^4.2.2",
        "symfony/filesystem": "^4.2.2"
    },
    "require-dev": {
      "phpunit/phpunit": "^7.5",
      "symfony/var-dumper": "^4.2.2",
      "phan/phan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\MercadopagoSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\MercadopagoSdk\\Tests\\": "tests"
        }
    },
    "bin": ["bin/mercadopago-sdk"],
    "scripts": {
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
