{
    "name": "gpupo/mercadopago-sdk",
    "description": "SDK Não Oficial para integração a partir de aplicações PHP com APIs Mercadopago",
    "homepage": "https://opensource.gpupo.com/mercadopago-sdk/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["mercadopago", "marketplace", "sdk", "api"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-bcmath": "*",
        "gpupo/common-sdk": "^4.0",
        "gpupo/common-schema": "^4.0",
        "symfony/yaml": "^4.0",
        "monolog/monolog": "^1.11",
        "symfony/finder": "^4.1",
        "symfony/cache": "^4.1"
    },
    "require-dev": {
      "phpunit/phpunit": "^7.2",
      "symfony/var-dumper": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\MercadopagoSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\MercadopagoSdk\\Tests\\": "tests"
        }
    },
    "bin": ["bin/mercadopago-sdk"],
    "scripts": {
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
